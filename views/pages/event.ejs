<%- include('../partials/_head', {title :'Event'}) %>
<%- include('../partials/_header') %>
<body>
    <h1><%= event.Name %></h1>
    <p><%= event.StartDate %> - <%= event.EndDate %></p>
    <h2>Contestants</h2>
    <ul>
        <% contestants.forEach((contestant)=> { %>
            <li>
                <h3>
                    <%= contestant.Name %>
                </h3>
                <p>
                    <%= contestant.Division %>
                </p>
                <p>
                    <%= contestant.Horse %>
                </p>
            </li>
            <hr />
        <% }) %>
    </ul>
</body>
<%- include('../partials/_footer') %>